<div class="centered_50">
<h1>Artist Sign Up</h1><br><br>
  <%= form_for @user do |f| %>
    <table class="centered_50">

<%= user = current_user %>
      <tr>
        <td>
          <%= f.label :User_name%>
        </td>
        <td>
          <%= f.text_field :name, class: "form-control" %>
        </td>
      </tr>

      <tr>
        <td>
          <%= f.label :email %>
        </td>
        <td>
          <%= f.email_field :email, class: "form-control" %>
        </td>
      </tr>

      <td>
        <%= f.label :password %>
      </td>
      <td>
        <%= f.password_field :password, class: "form-control" %>
      </td>
    </tr>

    <tr>
      <td>
        <%= f.label :password_confirmation %>
      </td>
      <td>
        <%= f.password_field :password_confirmation, class: "form-control" %>
      </td>
    </tr>
    <tr>
      <td>
        <br>
        <div class="text_align">
            <%= submit_tag "       Sign Up", class: 'button fix' %>
        </div>
      </td>
    </tr>
    </table>
  <% end %>


</div>

<div class="sign_in_music">

</div>


